<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Button name="RaidDkpButton" virtual="true">
        <Size>
            <AbsDimension x="52" y="45"/>
        </Size>
        <Scripts>
            <OnClick>
                    raidDkpButtonOnClick(self:GetID())
            </OnClick>
        </Scripts>
    </Button>
    <Frame name="Management" parent="OuterFrame">
        <Anchors>
            <Anchor point="TOPLEFT" relativeTo="OuterFrameList" relativePoint="TOPRIGHT">
                <Offset>
                    <AbsDimension x="0" y="-4"/>
                </Offset>
            </Anchor>
        </Anchors>
        <Size>
            <AbsDimension x="283" y="276"/>
        </Size>
        <Scripts>
            <OnLoad>
                createManagementButtons()
            </OnLoad>
        </Scripts>
        <Frames>
            <Frame name="TitleFrame">
                <Size>
                    <AbsDimension x="283" y="24"/>
                </Size>
                <Anchors>
                    <Anchor point="TOPLEFT">
                        <Offset>
                            <AbsDimension x="0" y="0"/>
                        </Offset>
                    </Anchor>
                </Anchors>
                <Backdrop bgFile="Interface\DialogFrame\UI-DialogBox-Background" tile="true">
                    <Color r="0" g="0" b="0" a="1"/>
                </Backdrop>
            </Frame>
        </Frames>
    </Frame>
</Ui>